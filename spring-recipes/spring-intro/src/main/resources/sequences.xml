	<bean id="sequenceGenerator" class="com.apress.springrecipes.sequence.SequenceGenerator">
		<property name="prefix" value="30" />
		<property name="suffix" value="A" />
		<property name="initial" value="100000" />
		<property name="suffixesList">
			<list>
				<value>A</value>
				<bean class="java.net.URL">
					<constructor-arg value="http" />
					<constructor-arg value="www.apress.com" />
					<constructor-arg value="/" />
				</bean>
				<null />
			</list>
		</property>
		<property name="suffixesSet">
			<set>
				<value>A</value>
				<bean class="java.net.URL">
					<constructor-arg value="http" />
					<constructor-arg value="www.apress.com" />
					<constructor-arg value="/" />
				</bean>
				<null />
			</set>
		</property>
		<property name="suffixesMap">
			<map>
				<entry key="type" value="A" />
				<entry key="url">
					<bean class="java.net.URL">
						<constructor-arg value="http" />
						<constructor-arg value="www.apress.com" />
						<constructor-arg value="/" />
					</bean>
				</entry>
			</map>
		</property>
		<property name="suffixesProperties">
			<props>
				<prop key="type">A</prop>
				<prop key="url">http://www.apress.com/</prop>
				<prop key="null">null</prop>
			</props>
		</property>
	</bean>

</beans>